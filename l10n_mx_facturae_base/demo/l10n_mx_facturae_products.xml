<!-- Product with IVA, RET ISR, RET IVA -->
        <record id="product_template_renta_oficina_demo_0" model="product.template">
            <field name="name">Arrendamiento de oficina</field>
            <field name="list_price">5355.0</field>
            <field name="standard_price">5355.0</field>
            <field name="type">service</field>
            <field name="uom_id" ref="product.product_uom_hour"/>
            <field name="uom_po_id" ref="product.product_uom_hour"/>
