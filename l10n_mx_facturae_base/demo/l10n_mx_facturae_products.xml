<?xml version="1.0" ?>
<openerp>
    <data noupdate="1">
<!-- Service with IVA Excento 0% -->
    <record id="product_template_agua_vauxoo_demo" model="product.template">
            <field name="name">Agua (Lt)</field>
            <field name="list_price">5.0</field>
            <field name="standard_price">5.0</field>
            <field name="type">consu</field>
            <field name="uom_id" ref="product.product_uom_unit"/>
            <field name="uom_po_id" ref="product.product_uom_unit"/>
            <field name="description_sale">Agua (Lt) DEMO</field>
        </record>
        
        <record id="product_product_agua_vauxoo_demo" model="product.product">
            <field name="product_tmpl_id" ref="product_template_agua_vauxoo_demo"/>
            <field name="taxes_id" eval="[(6,0,[ref('l10n_mx_facturae_base.l10n_mx_facturae_base_exento_iva0_mx')])]"/>
            <field name="default_code">Agua (Lt)</field>
            <field name="mes_type">fixed</field>
            <field name="uom_id" ref="product.product_uom_unit"/>
            <field name="type">consu</field>
            <field name="company_id" ref="base.main_company"/>
            <field name="name">Agua (Lt)</field>
            <field name="cost_method">standard</field>
            <field eval="1" name="seller_delay"/>
            <field name="categ_id" ref="product.product_category_7"/>
        </record>
