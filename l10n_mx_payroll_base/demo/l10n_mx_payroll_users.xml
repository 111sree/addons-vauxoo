<?xml version="1.0" ?>
<openerp>
    <data noupdate="1">
       
        <record id="payroll_mx_manager" model="res.users">
            <field name="name">Manager Electronic Payroll MX</field>
            <field name="login">payroll_mx_manager</field>
            <field name="password">1234</field>
            <field name="company_id" ref="base.main_company"/>
            <field name="company_ids" eval="[(4, ref('base.main_company'))]"/>
            <field name="tz">America/Mexico_City</field>
            <field name="menu_id" ref="base.action_menu_admin"/>
            <field name="groups_id" eval="[(6,0,[ref('base.group_user'), ref('base.group_partner_manager'),ref('l10n_mx_facturae_groups.group_l10n_mx_facturae_manager'),ref('base.group_hr_manager')])]"/>
        </record>

        <record id="payroll_mx_user" model="res.users">
            <field name="name">User Electronic Payroll MX</field>
            <field name="login">payroll_mx_user</field>
            <field name="password">1234</field>
            <field name="company_id" ref="base.main_company"/>
            <field name="company_ids" eval="[(4, ref('base.main_company'))]"/>
            <field name="tz">America/Mexico_City</field>
            <field name="groups_id" eval="[(6,0,[ref('base.group_partner_manager'),ref('l10n_mx_facturae_groups.group_l10n_mx_facturae_user'),ref('base.group_hr_user')])]"/>
        </record>
        
    </data>
</openerp>
