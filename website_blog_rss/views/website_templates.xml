<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
<template id="blog_rss_locs">
    <channel>
    <item t-foreach="posts" t-as="post">
        <title><t t-esc="post.name"/></title>
    </item>
    </channel>
</template>

<template id="blog_rss_xml">&lt;?xml version="1.0" encoding="UTF-8"?&gt;
<rss t-attf-xmlns="" version="2.0">
    <t t-raw="content"/>
</rss>
</template>

<template id="blog_rss_index_xml">&lt;?xml version="1.0" encoding="UTF-8"?&gt;
<sitemapindex t-attf-xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  <sitemap t-translate="off" t-foreach="pages" t-as="page">
    <loc><t t-esc="url_root"/>sitemap-<t t-esc="page"/>.xml</loc>
  </sitemap>
</sitemapindex>
</template>

    </data>
</openerp>