          <td>
            <para style="P43">Importe</para>
          </td>
        </tr>
      </blockTable>
      <section>
        <para style="terp_default_2">[[ repeatIn(o.invoice_line,'l') ]]</para>
        <blockTable colWidths="26.0,35.0,316.0,55.0,67.0" style="Table8">
          <tr>
            <td>
              <para style="P45">[[ formatLang(l.quantity)]]</para>
            </td>
            <td>
              <para style="P45">[[ (l.uos_id and l.uos_id.name) or '' ]]</para>
            </td>
            <td>
              <para style="P26">[[ l.name ]]</para>
            </td>
            <td>
              <para style="P44">[[ formatLang(l.price_unit) ]]</para>
            </td>
            <td>
              <para style="P44">[[ formatLang(l.price_subtotal, digits=get_digits(dp='Account')) ]] </para>
            </td>
          </tr>
          <tr>
            <td>
              <para style="P35">
                <font color="white"> </font>
              </para>
            </td>
            <td>
              <para style="P46">
                <font color="white"> </font>
              </para>
