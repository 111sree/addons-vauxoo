  <story>
    <section>
      <para style="P31">[[ repeatIn(objects,'o') ]] <font face="Helvetica">[[ set_global_data(o) ]]</font></para>
      <blockTable colWidths="76.0,292.0,131.0" style="Tabla1">
        <tr>
          <td>
            <para style="P32">[[ o.company_id.logo and setTag('para','image',{'width':'60.0','height':'90.6'}) ]] [[ o.company_id.logo ]]</para>
          </td>
          <td>
            <para style="P37">
              <font face="Helvetica" size="15.0">[[ (o.company_id and o.company_id.title and o.company_id.title.name) or '' ]] [[ (o.company_id and o.company_id.name) or '' ]]</font>
              <font face="Helvetica" size="15.0"/>
            </para>
            <para style="P39">Calle: [[ company_address()['street'] or '' ]] # [[ company_address()['street3'] or '' ]] </para>
            <para style="P39">Colonia: [[ company_address()['street2'] or '' ]], [[ company_address()['city2'] or '' ]]</para>
            <para style="P41">[[ company_address()['city'] or '' ]], [[ company_address()['state_id'] and company_address()['state_id']['name'] or '' ]] CP: [[ company_address()['zip'] or '' ]]</para>
            <para style="P41">RFC: <font face="Helvetica">[[ o.company_id.partner_id.vat ]]</font></para>
            <para style="P41">Teléfono(s): [[ company_address()['phone'] or removeParentNode('para') ]] Fax: [[ company_address()['fax'] or removeParentNode('para') ]]</para>
            <para style="P40">[[ o.company_id.partner_id.website]]</para>
          </td>
          <td>
            <para style="P4">
              <font face="Helvetica" size="12.0">Folio: [[ ( o.type in ['out_invoice', 'out_refund'] ) and ( o.state in ['open', 'paid', 'cancel'] ) and o.number or 'SIN FOLIO O ESTATUS NO VALIDO' ]]</font>
            </para>
            <para style="P5">
              <font color="white"> </font>
            </para>
            <para style="P6">[[ o.state == 'cancel' and 'FACTURA CANCELADA' ]]</para>
            <para style="P5">
              <font color="white"> </font>
            </para>
            <para style="P7">[[ company_address()['city'] or '' ]], </para>
            <para style="P7">[[ company_address()['state_id'] and company_address()['state_id']['name'] or '' ]] a</para>
            <para style="P7">[[ formatLang(o.date_invoice, date_time=True) ]]</para>
            <para style="P7">Serie: [[ get_approval() and get_approval().serie or '' ]] - Aprobación: [[ get_approval() and get_approval().approval_number or '' ]]</para>
            <para style="P7">Año Aprobación:[[ get_approval() and get_approval().approval_year or '' ]]</para>
            <para style="P7">
              <font color="white"> </font>
            </para>
            <para style="P22">Facturó: [[o.user_id.name]]</para>
          </td>
        </tr>
      </blockTable>
      <blockTable colWidths="215.0,153.0,132.0" repeatRows="1" style="Tabla2">
        <tr>
          <td>
            <para style="P10">Receptor</para>
            <para style="P12"><font face="Helvetica" size="6.0">Nombre</font><font face="Helvetica" size="7.0">:</font> [[ o.partner_id.name ]]</para>
            <para style="P29">Dirección: [[ o.address_invoice_id.street ]] No. Ext: [[ o.address_invoice_id.street3 ]] Int: [[ o.address_invoice_id.street4 ]]</para>
            <para style="P29">Colonia: [[ o.address_invoice_id.street2 or '' ]]</para>
            <para style="P29">Ciudad: [[ o.address_invoice_id.city or '' ]] Estado: [[ o.address_invoice_id.state_id and o.address_invoice_id.state_id.name or '' ]]</para>
            <para style="P29">Localidad: [[ o.address_invoice_id.city2 or '' ]] C.P.: [[ o.address_invoice_id.zip or '' ]] </para>
            <para style="P30">R. F. C. : [[ o.partner_id.vat ]]</para>
            <para style="P29">Teléfono(s): [[ o.address_invoice_id.phone or removeParentNode('para') ]] [[ o.address_invoice_id.fax or removeParentNode('para') ]] [[ o.address_invoice_id.mobile or removeParentNode('para') ]]</para>
          </td>
          <td>
            <para style="P10">Expedido en:</para>
            <para style="P10">[[ subcompany_address()['name'] or o.company_id.partner_id.name ]]</para>
            <para style="P10">Calle: [[ subcompany_address()['street'] or '' ]] Nro. Ext: [[ subcompany_address()['street3'] or '' ]] Int: [[ subcompany_address()['street4'] or '' ]]</para>
            <para style="P10">Colonia: [[ subcompany_address()['street2'] or '' ]]</para>
            <para style="P10">Ciudad: [[ subcompany_address()['city'] or '' ]] </para>
            <para style="P10">Estado: [[ subcompany_address()['state_id'] and subcompany_address()['state_id']['name'] or '' ]]</para>
            <para style="P10">Localidad: [[ subcompany_address()['city2'] or '' ]] CP: [[ subcompany_address()['zip'] or '' ]] </para>
          </td>
          <td>
            <para style="P38">Cliente No.: <font face="Helvetica" size="10.0">[[o.partner_id.ref]]</font></para>
            <para style="P38">Pedido No.: <font face="Helvetica" size="10.0">[[o.origin]]</font></para>
            <para style="P38">Fecha de Vencimiento: <font face="Helvetica" size="10.0">[[o.date_due]]</font></para>
          </td>
        </tr>
      </blockTable>
      <blockTable colWidths="27.0,36.0,314.0,55.0,66.0" style="Table7">
        <tr>
          <td>
            <para style="P42">Cantidad</para>
          </td>
