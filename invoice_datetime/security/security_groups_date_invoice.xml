<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">
        
        <record model="ir.module.category" id="module_date_invoice">
            <field name="name">Fecha FacturaE</field>
            <field name="description">Indicate tue type of date that need was colocated in the invoice</field>
            <field name="sequence">999</field>
        </record>
        
        <record id="group_date_invoice" model="res.groups">
            <field name="name">Date</field>
            <field name="comment">Date</field>
            <field name="category_id" ref="module_date_invoice"/>
        </record>
        
        <record id="group_datetime_invoice" model="res.groups">
            <field name="name">DateTime</field>
            <field name="comment">Date Time</field>
            <field name="category_id" ref="module_date_invoice"/>
            <field name="users" eval="[(4, ref('base.user_root'))]"/>
        </record>
        
        <record id="group_datetime_invoice" model="res.groups">
            <field name="implied_ids" eval="[(4, ref('group_datetime_invoice'))]"/>
        </record>
        
    </data>
</openerp>
