<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record model="ir.ui.view" id="view_hr_contract_working_day_form_inherit">
            <field name="name">view.hr.contract.working.day.form.inherit</field>
            <field name="model">hr.contract</field>
            <field name="inherit_id" ref="hr_contract.hr_contract_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='working_hours']" position="after">
                    <field name="working_day_id"/>
                </xpath>
                <xpath expr="//field[@name='wage']" position="before">
                    <field name="integrated_salary"/>
                </xpath>
            </field>
        </record>
    </data>
</openerp>
